<h1>Listing storages</h1>

<table>
  <tr>
    <th>id</th>
    <th>Name</th>
    <th>Type</th>
    <th>Disksize</th>
  </tr>

<% @storages.each do |storages| %>
  <tr>
    <td><%=h storages.HARDWARE_ID %></td>
    <td><%=h storages.NAME %></td>
    <td><%=h storages.TYPE %></td>
    <td><%=h storages.DISKSIZE %></td>
    <td><%= link_to 'Show', storages %></td>
  </tr>
<% end %>
</table>

<br />

